  color: #fff; }
.menu .sidebar .nav-heading {
  height: 40px;
  padding-top: 10px;
  padding-bottom: 5px;
  padding-left: 15px;
  color: #ffffff;
  cursor: pointer; }
.menu .nav-acitive {
  background-color: #222837;
