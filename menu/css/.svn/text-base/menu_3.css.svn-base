body, html {
  height: 100%; }

.menu .dropdown-menu {
  border-radius: 3px; }
.menu .dropdown-header {
  color: #a1a2a3; }
.menu .wrapper {
  position: relative;
  width: 100%;
  height: auto;
  min-height: 100%;
  overflow-x: hidden; }
.menu .wrapper > .aside {
  position: absolute;
  width: 220px;
  top: 0;
  left: 0;
  bottom: 0;
  z-index: 116;
  -webkit-backface-visibility: hidden;
  -moz-backface-visibility: hidden;
  backface-visibility: hidden;
  background-color: #fff; }
.menu .wrapper > .aside .aside-inner {
  padding-top: 110px;
  height: 100%;
  width: 220px;
  overflow: hidden; }
.menu .wrapper > section {
  position: relative;
  height: 100%;
  margin-left: 0;
  z-index: 111;
  background-color: #f5f7fa;
  margin-bottom: 60px !important; }
.menu .modal-open .wrapper {
  z-index: 0; }
@media only screen and (min-width: 768px) {
  .menu body {
    min-height: 100%; }
  .menu .wrapper > .aside .aside-inner {
    padding-top: 55px; }
  .menu .wrapper > section {
    margin-left: 220px; } }
.menu .sidebar {
  height: 100%;
  padding-bottom: 20px;
  background-color: #fff;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch; }
.menu .sidebar:after {
  content: "";
  background: rgba(0, 0, 0, 0.15);
  position: absolute;
  display: block;
  top: 0;
  bottom: 0;
  right: 0;
  width: 1px;
  -webkit-transform: translateZ(0); }
.menu .sidebar .nav-heading {
  padding: 12px 15px;
  color: #919da8;
  font-size: 13px;
  letter-spacing: .035em;
  cursor: default; }
@media only screen and (min-width: 1025px) {
  .menu .sidebar {
    margin-right: -17px;
    overflow-y: scroll; } }
.menu .sidebar {
  background-color: #313a4f; }
.menu .nav-second {
  font-size: 14px;
  margin-top: 3px;
  color: #b1b1b1; }
.menu .nav-second:hover {
  color: #5d9cec; }
.menu .nav-second-row {
  padding-top: 10px;
  padding-bottom: 5px;
  padding-left: 15px;
  color: #b1b1b1; }
.menu .nav-second-row:hover {
  color: #5d9cec; }
.menu .handing-name {
  margin-top: 3px;
  font-size: 14px; }
.menu .handing-name:hover {
  color: #5d9cec; }
.menu .nav-item .row, .menu .nav-content .row {
