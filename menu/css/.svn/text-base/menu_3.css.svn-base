.menu .pull-btn {
  padding-left: 0px;
  margin-top: -1px; }
.menu .upIcon {
  margin-top: -8px; }
.menu .transDownIcon {
  transform-origin: center center;
  transform: rotate(90deg);
  -ms-transform: rotate(90deg);
  -moz-transform: rotate(90deg);
  -webkit-transform: rotate(90deg);
  -o-transform: rotate(90deg);
  transition: .2s; }
.menu .transUpIcon {
  transform-origin: center center;
