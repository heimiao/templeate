.left {
  float: left;
}

.right {
  float: right;
}

.left-menu {
  margin-top: 22px;
}
.left-menu span {
  display: block;
  line-height: 40px;
}
.left-menu span > a {
  cursor: pointer;
  padding-left: 25px;
  font-size: 16px;
  width: 100%;
  height: 100%;
  display: inline-block;
  color: #819aaa;
  text-decoration: none;
}
.left-menu span img {
  margin-right: 15px;
}
.left-menu span img[class="right"] {
  margin-right: 25px;
  margin-top: 18px;
}
.left-menu ul {
  padding: 0px;
  margin: 0px;
  background: #35404d;
}
.left-menu ul li {
  line-height: 40px;
}
.left-menu ul li span > a {
  cursor: pointer;
  color: #819aaa;
  padding-left: 50px;
  font-size: 14px;
  width: 100%;
  height: 100%;
  display: inline-block;
  text-decoration: none;
}
.left-menu ul li ul li a {
  cursor: pointer;
  padding-left: 85px;
  width: 100%;
  height: 100%;
  display: inline-block;
  text-decoration: none;
}
.left-menu ul li ul li a:hover {
  color: #fff;
  background: #27a9e3;
}
.left-menu ul li ul li a:hover img {
  display: block;
}
.left-menu ul li ul li img[class="right"] {
  margin-top: 12px;
}

.menuIn {
  color: #fff;
  background: #27a9e3;
}

.menuOut {
  background: #35404d;
  color: #819aaa;
}

