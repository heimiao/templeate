			            },
			            trad_time: {
			                validators: {
			                    notEmpty: {
			                        message: '交易时间不能为空'
			                    } 
			                }
			            },
			            trad_buyprice: {
			                validators: {
			                    notEmpty: {
			                        message: '回购价格不能为空'
			                    },
			                    regexp: {
			                        enabled: true,
			                        regexp: /^[1-9]d*.d*|0.d*[1-9]d*|0?.0+|0$/i,
			                        message: '输入内容只能含有数字和小数点且不少于两位'
			                    }
			                }
			            },
			            trad_ll: {
			                validators: {
			                    notEmpty: {
			                        message: '回购利率不能为空'
			                    } 
			                }
			            },trad_zhu: {
			                validators: {
			                    notEmpty: {
			                        message: '董事会主席不能为空'
			                    } 
			                }
			            },trad_manager: {
