			            },
			            trad_gu: {
			                validators: {
			                    notEmpty: {
			                        message: '投后估值不能为空'
			                    },
			                    regexp: {
			                        enabled: true,
			                        regexp: /^[1-9]d*.d*|0.d*[1-9]d*|0?.0+|0$/i,
			                        message: '输入内容只能含有数字和小数点且不少于两位'
			                    }  
			                }
			            },
			            trad_dong: {
			                validators: {
			                    notEmpty: {
			                        message: '出让股东不能为空'
			                    } 
			                }
			            },
			            trad_num: {
			                validators: {
			                    notEmpty: {
			                        message: '董事数量不能为空'
			                    },
			                    regexp: {
			                        enabled: true,
			                        regexp: /[1-9]$/i,
			                        message: '输入内容只能有数字'
			                    }  
			                }
			            },
			            trad_jian: {
			                validators: {
			                    notEmpty: {
