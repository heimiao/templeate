		<script type="text/javascript">
		//校验
		$(document).ready(function() {
			$('#userForm').formValidation({
			        err: {
			            container: 'tooltip'
			        },
			        icon: {
			            valid: 'glyphicon glyphicon-ok',
			            invalid: 'glyphicon glyphicon-remove',
			            validating: 'glyphicon glyphicon-refresh'
			        },
			        fields: {
			            famcname: {
			                validators: {
			                    notEmpty: {
			                        message: '请输入方案名称'
			                    }
			                }
			            },
			            trad_price: {
			                validators: {
			                    notEmpty: {
			                        message: '每股价格不能为空'
			                    },
			                    regexp: {
			                        enabled: true,
			                        regexp: /^[1-9]d*.d*|0.d*[1-9]d*|0?.0+|0$/i,
			                        message: '输入内容只能含有数字和小数点且不少于两位'
			                    }  
			                }
			            },
			            trad_total: {
			                validators: {
			                    notEmpty: {
