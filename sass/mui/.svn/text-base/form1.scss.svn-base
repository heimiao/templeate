   .Two_select{
        .selectc {
        border: 1px solid gainsboro;
        width:30%;
        min-width:100px;
        vertical-align: middle;
        } 
        .selectc:active {
            outline: none;
        } 
        .role_arrow {
            vertical-align: middle;
            position: relative;
        }
        .role_arrow img{
             width:10%;
        }
        .role_arrow span {
            position: absolute;
            display: inline-block;
            height: 100%;
            width: 45%;
            cursor: pointer;
            background:#fff;
            @include opacity(0);
        } 
        .left_arrow {
            left: 0px;
            top: 0px;
        }
        .right_arrow {
            right: 0px;
            top: 0px;
        }
   }
