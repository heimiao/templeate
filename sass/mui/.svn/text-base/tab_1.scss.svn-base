@charset "utf-8";
.tabControl{
    .tabBtn{ 
            padding:0px;
            margin:0px;
            font-size:0px;
            padding-left:1px;
            li{
                list-style-type: none;
                height:30px;
                line-height: 30px;
                min-width: 80px;
                text-align: center;
                display:inline-block;
                cursor: pointer;
                background: #EFF2F9;
                padding:0px 15px;
                font-family: "微软雅黑";
                font-size: 14px;
                border: 1px #DDDDDD solid;
                margin-left:-1px; 
                >a{
                    color:#000;
                    text-decoration:none;
                }
                &:hover{
                    background: #fff;
                     border-bottom:0px;
                } 
                .warning_circle_icon,.error_circle_icon,.info_circle_icon,.success_circle_icon{
                    &:before{ 
                        display:inline-block;
                        margin-right:5px;
                        height:100%;
                        content:"";
                    }
                }
                .warning_circle_icon{ &:before{content:url("../tab/img/circle1_icon.jpg");}}
                .error_circle_icon{&:before{content:url("../tab/img/circle2_icon.jpg");}}  
                .info_circle_icon{&:before{content:url("../tab/img/circle3_icon.jpg");}}
                .success_circle_icon{&:before{content:url("../tab/img/circle1_icon.jpg");}} 
            }
            .checked{
                background: #fff;
                border-bottom:0px;
            }
        }
        .tabCont{  
           width:100%;
           min-height:100px; 
           border:1px solid #ddd;
           margin-top:-1px;
           >section{
               display:none; 
           }
        }
}


