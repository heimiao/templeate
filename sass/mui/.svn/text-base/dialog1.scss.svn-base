@charset "utf-8";

/*设置dialog_1样式*/

.dialog_header{
    background: #fff !important;
}
.modal-con{
    border: 6px solid;
    border-color: rgba(127,127,127,0.3);
    width: 405px;
    margin-left: auto;
    margin-right: auto;
}
.dia_block{
    min-width: 600px;
    .dia_header{
        height: 45px;
        line-height: 45px;
        border-top-left-radius: 5px;
        border-top-right-radius: 5px;
        background: #2E363F;
        
        span{
            color: #fff;
            cursor: pointer;
        }
        .dh_cont{
            float: left;
            margin-left: 20px; 
            color:#000;
            font-weight:1000;
        }
        .dh_close{
            float: right;
            margin-right: 20px;
        }
    }
    .dia_body{
        min-height:100px;
        background: #fff;
        border-bottom-left-radius: 5px;
        border-bottom-right-radius: 5px;
        padding: 10px;
    }
}
